<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:ImageCollection">
    <SolidColorBrush x:Key="Base.Foreground" Color="White"/>
    <SolidColorBrush x:Key="Menu.Background" Color="#ff191919"/>
    <SolidColorBrush x:Key="Menu.BorderBrush" Color="#ff191919"/>
    <SolidColorBrush x:Key="Menu.Separator" Color="White"/>
    <SolidColorBrush x:Key="Highlighted.BorderBrush" Color="#ff999999"/>
    <SolidColorBrush x:Key="Highlighted.Background" Color="#ff999999"/>
    <SolidColorBrush x:Key="Menu.CheckMark" Color="DarkGray"/>
    <SolidColorBrush x:Key="Menu.InputGestureText" Color="DarkGray"/>
    <SolidColorBrush x:Key="ComboBox.Background" Color="#ff191919"/>
    <SolidColorBrush x:Key="ComboBox.BorderBrush" Color="#ff191919"/>
    <SolidColorBrush x:Key="Base.MouseOver.Background" Color="#ff999999"/>
    <SolidColorBrush x:Key="Base.MouseOver.BorderBrush" Color="#ff999999"/>
    <SolidColorBrush x:Key="Base.Pressed.Background" Color="#ff999999"/>
    <SolidColorBrush x:Key="Base.Pressed.BorderBrush" Color="#ff999999"/>
</ResourceDictionary>